{
    "name": "kirschbaum-development/nova-mail",
    "description": "A Laravel Nova action for sending mail.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "KirschbaumDevelopment\\NovaMail\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KirschbaumDevelopment\\NovaMail\\NovaMailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}